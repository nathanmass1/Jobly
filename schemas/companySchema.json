{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "handle"
    ],
    "properties": {
      "handle": {
        "$id": "#/properties/handle",
        "type": "string",
        "title": "The Handle Schema",
        "default": "",
        "examples": [
          "twitter"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Twitter Inc."
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/num_employees",
        "type": "integer",
        "title": "The Num_employees Schema",
        "default": 0,
        "examples": [
          5000
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Tweet thingy"
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logo_url",
        "type": "string",
        "title": "The Logo_url Schema",
        "default": "",
        "examples": [
          "wwww.google.com"
        ],
        "pattern": "^(.*)$"
      }
    }
  }